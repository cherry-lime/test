{
  "name": "advisor-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.11.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "eslint-import-resolver-typescript": "^2.7.1",
    "@vitejs/plugin-react": "^1.3.2",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "vitest": "^0.14.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "cd .. && husky install advisor-frontend/.husky",
    "lint": "npx eslint \"./src/**/*.{ts,tsx,js,jsx}\"",
    "prettier-write": "npx prettier --write \"./src/**/*.{ts,tsx,js,jsx}\"",
    "prettier-check": "npx prettier --check \"./src/**/*.{ts,tsx,js,jsx}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "lint-staged": "^12.4.1",
    "prettier": "2.6.2",
    "@types/react": "^17.0.45",
    "c8": "^7.11.3",
    "jsdom": "^19.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.3.2",
    "husky": "^8.0.1"
  }
}
